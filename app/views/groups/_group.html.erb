<%= group.name %> </br>
<%= group.description %>
<ul>
<% group.users.each do |user| %>
	<li> <%= user.name %> </li>
<% end %>
<% if group.users.include? current_user %>
	<%= button_to "Leave Group", leave_group_path(:group_id => group.id), :method => :patch) %>
<% else %>
 	<%= button_to "Join Group", join_group_path(:group_id => group.id), :method => :patch) %>
<% end %>
</ul>